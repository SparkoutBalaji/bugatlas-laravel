{
    "name": "balaji13011/bug-atlas",
    "description": "",
    "keywords": [
        "sparkouttech",
        "bug-atlas"
    ],
    "homepage": "https://github.com/sparkouttech/bug-atlas",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "type": "library",
    "authors": [
        {
            "name": "Sparkout",
            "email": "developer@sparkouttech.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^7.3|^7.4|^8.0|^8.1|^8.2|^8.3"
    },
   
    "autoload": {
        "psr-4": {
            "Sparkouttech\\BugAtlas\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sparkouttech\\BugAtlas\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sparkouttech\\BugAtlas\\BugAtlasServiceProvider",
                "Sparkouttech\\BugAtlas\\BugAtlasExceptionServiceProvider"
            ],
            "aliases": {
                "BugAtlas": "Sparkouttech\\BugAtlas\\BugAtlasFacade"
            }
        }
    }
}
