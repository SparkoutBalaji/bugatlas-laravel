{
    "name": "balaji13011/bug-atlas",
    "description": "",
    "keywords": [
        "sparkouttech",
        "bug-atlas"
    ],
    "homepage": "https://github.com/sparkouttech/bug-atlas",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Sparkout",
            "email": "developer@sparkouttech.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Sparkouttech\\BugAtlas\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sparkouttech\\BugAtlas\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sparkouttech\\BugAtlas\\BugAtlasServiceProvider"
            ],
            "aliases": {
                "BugAtlas": "Sparkouttech\\BugAtlas\\BugAtlasFacade"
            }
        }
    }
}
